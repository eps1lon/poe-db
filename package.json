{
  "name": "poedb-node",
  "version": "0.0.1",
  "description": "relational db backend for PoE's content.ggpk",
  "main": "index.js",
  "repository": {
    "url": "https://github.com:eps1lon/poe_db_node.git",
    "type": "git"
  },
  "author": "eps1lon",
  "license": "MIT",
  "scripts": {
    "db:drop": "node scripts/db/drop",
    "db:create": "node scripts/db/create",
    "db:insert": "node scripts/db/insert",
    "db:build": "yarn run db:create",
    "db:models": "node --trace-warnings scripts/generateModels",
    "postdb:models": "prettier-eslint --write \"src/models/*.js\"",
    "db:rebuild": "yarn run db:drop && yarn run db:build",
    "db:schema": "node scripts/db/schema",
    "install": "cd pypoe && pip install -e .[cli]",
    "precommit": "lint-staged",
    "test": "jest",
    "parse-content": "pypoe_exporter dat json --include-virtual-fields data/content.json",
    "postparse-content": "node scripts/postParseContent",
    "format": "prettier-eslint --write \"src/**/*.js\" \"scripts/**/*.js\""
  },
  "lint-staged": {
    "*.js": ["prettier-eslint --write", "git add"]
  },
  "jest": {
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(js)$"
  },
  "devDependencies": {
    "eslint": "^4.0.0",
    "eslint-config-prettier": "^2.2.0",
    "eslint-plugin-prettier": "^2.1.2",
    "husky": "^0.14.0",
    "jest": "^20.0.4",
    "lint-staged": "^4.0.0",
    "prettier": "^1.5.2",
    "prettier-eslint": "^6.4.2",
    "prettier-eslint-cli": "^4.1.1"
  },
  "dependencies": {
    "babel-core": "^6.25.0",
    "babel-types": "^6.25.0",
    "mysql2": "^1.3.6",
    "sequelize": "^4.3.1",
    "string": "^3.3.3",
    "yargs": "^8.0.2"
  }
}
