{
  "name": "poedb-node",
  "version": "0.0.1",
  "description": "relational db backend for PoE's content.ggpk",
  "main": "index.js",
  "repository": {
    "url": "https://github.com:eps1lon/poe_db_node.git",
    "type": "git"
  },
  "author": "eps1lon",
  "license": "MIT",
  "scripts": {
    "db:drop": "node scripts/db/drop",
    "db:create": "node scripts/db/create",
    "db:insert": "node scripts/db/insert",
    "db:build": "yarn run db:create && yarn run db:insert",
    "db:rebuild": "yarn run db:drop && yarn run db:build",
    "dev:server": "nodemon server.js",
    "dev:models": "node --trace-warnings scripts/generateModels",
    "postdev:models": "prettier-eslint --write \"src/models/*.js\"",
    "dev:parseContent": "pypoe_exporter dat json --include-virtual-fields data/content.json",
    "postdev:parseContent": "node scripts/postParseContent",
    "install": "cd pypoe && pip install -e .[cli]",
    "lint": "prettier-eslint",
    "precommit": "lint-staged",
    "start": "node server.js",
    "test": "jest",
    "format": "prettier-eslint --write \"src/**/*.js\" \"scripts/**/*.js\""
  },
  "lint-staged": {
    "*.js": [
      "prettier-eslint --write",
      "git add"
    ]
  },
  "jest": {
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(js)$"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-types": "^6.25.0",
    "eslint": "^4.0.0",
    "eslint-config-prettier": "^2.2.0",
    "eslint-plugin-prettier": "^2.1.2",
    "husky": "^0.14.0",
    "jest": "^20.0.4",
    "lint-staged": "^4.0.0",
    "lodash": "^4.17.4",
    "nodemon": "^1.11.0",
    "prettier": "^1.5.2",
    "prettier-eslint": "^6.4.2",
    "prettier-eslint-cli": "^4.1.1"
  },
  "dependencies": {
    "express": "^4.15.3",
    "inflection": "^1.12.0",
    "mysql2": "^1.3.6",
    "sequelize": "^4.3.1",
    "string": "^3.3.3"
  }
}
